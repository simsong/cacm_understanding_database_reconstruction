(= (+ (if (and (= S1 0) (> A1 30)) 1 0)
      (if (and (= S2 0) (> A2 30)) 1 0)      
      (if (and (= S3 0) (> A3 30)) 1 0)      
      (if (and (= S4 0) (> A4 30)) 1 0)      
